// Hashmap Method
var missingNumber = function(nums) {
    const numsMap = new Map()
    for (let i = 0; i < nums.length; i++) {
        numsMap.set(nums[i], true)
    }
    for (let i = 0; i < nums.length; i++) {
        if (numsMap.get(i) == undefined) {
            return i
        }
    }
};

